{
  "name": "@envguard/core",
  "version": "0.1.0",
  "private": true,
  "description": "Core business logic for EnvGuard - internal package",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist coverage .turbo"
  },
  "dependencies": {
    "@napi-rs/keyring": "1.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.19.22",
    "@vitest/coverage-v8": "^1.6.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "envguard",
    "secrets",
    "keychain",
    "core",
    "internal"
  ],
  "license": "MIT"
}
